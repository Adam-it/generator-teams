{
  "name": "<%= packageName %>",
  "version": "0.0.1",
  "description": "<%= description %>",
  "scripts": {
    "start": "node dist/server.js",
    "build": "gulp build",
    "debug": "gulp serve --debug",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@microsoft/teams-js": "1.4.1",
    "@types/debug": "0.0.31",
    "@types/express": "4.16.0",
    "@types/express-session": "1.15.10",
    "@types/morgan": "1.7.35",
    "@types/request": "2.47.1",
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^24.0.11",
    "@types/react-test-renderer": "^16.8.1",
    "botbuilder": "4.3.2",
    "botbuilder-dialogs": "4.3.2",
    "botbuilder-teams": "4.0.0-beta1",
    "botframework-config": "4.3.2",
    "browser-request": "0.3.3",
    "debug": "^3.1.0",
    "dotenv-webpack": "^1.7.0",
    "del": "^3.0.0",
    "express": "4.16.3",
    "express-msteams-host": "1.3.0-preview4",
    "express-session": "1.15.6",
    "dotenv": "6.1.0",
    "fancy-log": "1.3.2",
    "gulp": "4.0.0",
    "gulp-inject": "4.3.2",
    "gulp-token-replace": "1.1.2",
    "gulp-zip": "4.2.0",
    "morgan": "1.9.1",
    "nodemon": "1.18.3",
    "plugin-error": "1.0.1",
    "request": "2.88.0",
    "ts-loader": "5.3.3",
    "typescript": "3.3.4000",
    "vinyl": "2.2.0",
    "tslint": "^5.11.0",
    "tslint-webpack-plugin": "^1.2.2",
    "webpack": "4.26.0",
    "yargs": "12.0.1",
    "z-schema": "^3.19.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "jest": "^24.5.0",
    "react-test-renderer": "^16.8.5",
    "tslint-loader": "^3.6.0"
  }, 
  "devDependencies": {
   
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ],
    "collectCoverageFrom": [
      "src/app/scripts/**/*.{js,jsx,ts,tsx}",
      "!src/app/scripts/client.ts",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}