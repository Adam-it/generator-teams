{
  "name": "<%= packageName %>",
  "version": "0.0.1",
  "description": "<%= description %>",
  "scripts": {
    "start": "node dist/server.js",
    "build": "gulp build",
    "debug": "gulp serve --debug"<% if (unitTestsEnabled) { %>,
    "test": "jest",
    "coverage": "jest --coverage"<%}%>
  },
  "dependencies": {
    "@microsoft/teams-js": "1.8.0",
    "axios": "^0.19.2",
    "botbuilder": "4.10.4",
    "botbuilder-dialogs": "4.10.4",
    "compression": "1.7.4",
    "debug": "4.2.0",
    "dotenv": "8.2.0",
    "express": "4.16.4",
    "express-msteams-host": "1.7.0",
    "express-session": "1.15.6",
    "morgan": "1.9.1"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/express": "4.16.0",
    "@types/express-session": "1.15.10",
    "@types/morgan": "1.7.35",
    <% if (unitTestsEnabled) { %>"@types/enzyme": "^3.9.1",
    "@types/jest": "^26.0.15",
    "@types/enzyme-to-json": "^1.5.3",<%}%>
    "browser-sync": "^2.26.5",
    "dotenv-webpack": "^3.0.0",
    <% if (unitTestsEnabled) { %>"enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",<%}%>
    "fancy-log": "^1.3.3",
    "gulp": "4.0.2",
    <% if (unitTestsEnabled) { %>
    "jest": "^26.6.1",
    "ts-jest": "^26.4.3",
    <%}%>
    "ngrok": "^3.1.1",
    "ts-loader": "8.0.5",
    "tslint-webpack-plugin": "^1.2.2",
    "tslint": "^5.11.0",
    "typescript": "4.0.3",
    "vinyl": "2.2.0",
    "webpack": "5.0.0",
    "yargs": "^16.0.3",
    "tslint-loader": "^3.6.0",
    "yoteams-build-core": "1.0.0-preview",
    "webpack-node-externals": "^2.5.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ]<% if (unitTestsEnabled) { %>,
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ],
    "collectCoverageFrom": [
      "src/app/scripts/**/*.{js,jsx,ts,tsx}",
      "!src/app/scripts/client.ts",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "text"
    ]
  }<%}%>
}